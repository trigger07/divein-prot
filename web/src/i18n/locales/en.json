  "mockExperience": {
    "title": "Gastronomic Tour of Barcelona's Local Markets",
    "description": "Discover the authentic flavors of Barcelona on this tour of the city's most emblematic markets. Accompanied by a local guide who is an expert in gastronomy, you will visit the famous La Boqueria Market and other lesser-known but equally fascinating markets.\n\nYou will taste a selection of traditional tapas, Iberian cold cuts, artisanal cheeses, and typical sweets. You will learn about the culinary history of Catalonia and the preparation techniques of the most representative dishes.\n\nThis experience includes stops at small family stores that have maintained their recipes for generations, where you can taste fresh products and talk directly with local producers.",
    "languages": {
      "spanish": "Spanish",
      "english": "English",
      "catalan": "Catalan",
      "french": "French"
    },
    "location": "Barcelona, Spain",
    "included": {
      "item1": "Tasting of 8 different tapas",
      "item2": "Drinks (water, wine or soft drink)",
      "item3": "Local gastronomic guide",
      "item4": "Digital recipe book of Catalan dishes"
    },
    "excluded": {
      "item1": "Transportation to/from the meeting point",
      "item2": "Tips (optional)",
      "item3": "Personal purchases in the markets"
    }
